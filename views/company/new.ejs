<% layout("/layouts/boilerplate")%>
<body>
    <div class="row">
        <div class="col-8 offset-2">
            <br><br>
    <h1>Add New Company</h1>
    <form method="POST" action="/listings" enctype="multipart/form-data">
        <div class="mb-3">
        <label for="name" class="form-label" >Name</label>
        <input type="text" required placeholder="Enter Company name" name="company[name]" class="form-control">
        </div>
        <div class="mb-3">
        <label for="from" class="form-label">From</label>
        <input type="number" required placeholder="From" name="company[from]" class="form-control">
         </div>
         <div class="mb-3">
         <label for="to" class="form-label">To</label>
        <input type="number" required placeholder="To" name="company[to]" class="form-control">
         </div>
         <div class="mb-3">
         <label for="areport" class="form-label">Annual Report</label>
        <input type="file" required name="company[areport]" class="form-control">
         </div>
         <div class="mb-3">
            <br>
        <button class="btn add-btn">Add</button><br><br><br>
    </form>
</body>
</div>
</div>
</html>